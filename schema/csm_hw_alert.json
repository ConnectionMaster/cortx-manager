{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "id": "http://json-schema.org/draft-03/schema#",
    "title": "CSM Schema",

    "type": "object"
    "properties": {
        "header": {
            "type": "hw",
            "alert_type": "missing",
            "status": "not present",
            "resolved": "no",
            "acknowledged": "no",
            "description": "The power supply is not installed",
            "severity": "1",
            "recommendation": "If the power supply is missing, insert a power supply of the correct type.  - If the power supply is present, confirm that it is fully inserted and locked in place.  - If the above steps do not resolve the issue, the power supply has probably failed and if the error persists, it should be replaced."
        },
        "hw": {
            "vendor": "",
            "enclosure_id": 0,
            "serial_number": "",
            "part_number": ""
       },
}

{
    "$schema": "http://json-schema.org/draft-03/schema#",
    "id": "http://json-schema.org/draft-03/schema#",
    "title": "CSM Schema",

    "type": "object"
    "properties": {
        "header": {
            "type": "sw",
            "alert_type": "disabled",
            "status": "not running",           
            "resolved": "no",
            "acknowledged": "no",
            "description": "",
            "severity": "1",
            "recommendation": ""
        },
        "sw": {
            "node_id": "",
            "component": "",
            "module": "",
            "error_code": ""
       },
}
